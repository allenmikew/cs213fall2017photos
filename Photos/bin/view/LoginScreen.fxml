<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginViewController">
   <children>
      <Button fx:id="loginButton" layoutX="275.0" layoutY="188.0" mnemonicParsing="false" onAction="#login" text="Log In" />
      <PasswordField fx:id="passwordField" layoutX="288.0" layoutY="151.0" />
      <TextField fx:id="usernameField" layoutX="288.0" layoutY="113.0" />
      <Label layoutX="205.0" layoutY="117.0" text="Username" />
      <Label layoutX="207.0" layoutY="155.0" text="Password" />
      <Label layoutX="242.0" layoutY="43.0" text="Photos">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Label layoutX="273.0" layoutY="327.0" text="New user?" />
      <Button fx:id="createNewUserButton" layoutX="250.0" layoutY="352.0" mnemonicParsing="false" onAction="#addUser" text="Create new user" />
   </children>
</AnchorPane>
